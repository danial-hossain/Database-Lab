create database exampleDatabase


create table Person(
  P_Id int,
  FirstName varchar(200),
  LastName varchar(200),
  Address varchar(200),
  city varchar(200)
);

insert into Person values (1,'Abdur','Rahim','Mirpur','Dhaka' )
insert into Person values (2,'Danial','Hossain','Gulshan','Dhaka' )
insert into Person values (3,'Abdur','X','Mirpur','Dhaka' )
insert into Person values (4,'Abdur','Y','Mirpur','Dhaka' )
insert into Person values (5,'Abdur','M','Mirpur','Dhaka' )
insert into Person values (16,'Abdur','R','Mirpur','Dhaka' )


insert into Person (P_Id,FirstName,Address,city) values (6,'Abdur5','Rahim5','Server')

select * from Person

select distinct Address from person; 

select city from Person where P_Id= 1;

select * from Person where city = 'Dhaka'
select * from Person where FirstName='Abdur' or city='Dhaka'

alter table Person add age int;
select * from Person


update Person 
set LastName ='rahim'
where P_Id=6

select * from Person 

delete Person 
where P_Id=7
select * from Person  where P_Id between 1 and 6

select * from Person where FirstName='Abdur' or city='Mirpur'

select * from Person where Address like '%pur' 
